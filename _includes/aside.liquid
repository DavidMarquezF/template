<div class="sideaffix">
  {%- unless disableContribution -%}
  <div class="contribution">
    <ul class="nav">
      <li>
        <a id="switch-theme" class="contribution-link">
          <span class="glyphicon glyphicon-cog"></span>Theme
        </a>
      </li>
      {%- if page.original_content_git_url -%}
      <li>
        <a href="{{page.original_content_git_url}}" class="contribution-link">
          <span class="glyphicon glyphicon-pencil"></span>Edit this doc
        </a>
      </li>
      {%- endif -%}
    </ul>
  </div>
  {%- endunless -%}
  <nav class="bs-docs-sidebar hidden-print hidden-xs hidden-sm affix" id="affix">
  </nav>
</div>