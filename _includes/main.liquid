<!DOCTYPE html>
<html>

<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  {{metadata}}

  <title>{{ page.title }}</title>

  <link rel="stylesheet" href="{{ page._rel }}dist/docfx.css">
  <script type="text/javascript" src="{{ page._rel }}dist/docfx.js"></script>
</head>

<body data-spy="scroll" data-target="#aside" data-offset="120">
  <div id="wrapper">
    <header>
      {%- include header -%}

      {%- unless disableBreadcrumb -%}
      <nav class="subnav navbar navbar-default">
        <div class="container" id="breadcrumb"></div>
      </nav>
      {%- endunless -%}
    </header>

    <main class="container page-type-{{pageType}}">
      {%- if fullPage -%}
        {{ content }}
      {%- else -%}
        <nav class="sidenav">
          {%- include toc -%}
        </nav>

        <div class="article row grid-right">

          <article class="content wrap col-md-10" data-uid="{{uid}}">
            {{ heading1 }}
            {%- include metabar -%}
            {{ content }}
          </article>

          <aside class="hidden-sm col-md-2">
            {%- include aside -%}
          </aside>
        </div>
      {%- endif -%}
    </main>

    <footer>
      {%- include footer -%}
    </footer>
  </div>
</body>

</html>